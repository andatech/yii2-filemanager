{
    "name": "andatech/yii2-filemanager",
    "description": "Yii 2 filemanager with CKEditor or TinyMCE",
    "type": "yii2-extension",
    "keywords": ["yii2","extension"],
    "license": "GPL-3.0",
    "minimum-stability": "stable",
    "support": {
        "source": "https://github.com/andatech/yii2-filemanager.git",
        "issues": "https://github.com/andatech/yii2-filemanager/issues"
    },
    "authors": [
        {
            "name": "Anda Tech",
            "email": "kuakling@gmail.com",
            "role" : "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "sunhater/kcfinder": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "anda\\filemanager\\": ""
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
